#include<stdio.h>
#include<queue>
#include"../namespace/namespace.hpp"

#ifndef __PIXEL
#define __PIXEL
typedef struct pxl{
	uint8_t level;
	unsigned char value;
	pxl(){
		level = 0;
		value = 0;
	}
}pxl;
#endif

extern queue<signed char*> video_codec;
